{
  "name": "boycott",
  "version": "0.1.0",
  "description": "A map app",
  "repository": "git@github.com:lucifer1004/boycott.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "license": "MIT",
  "dependencies": {
    "@lucifer1004/react-google-map": "2.1.0",
    "lodash": "4.17.11",
    "navi": "0.10.13",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-navi": "0.10.13"
  },
  "devDependencies": {
    "@types/googlemaps": "3.30.16",
    "@types/jest": "23.3.13",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.20",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.8.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.1",
    "navi-scripts": "0.10.13",
    "prettier": "1.16.3",
    "react-scripts": "2.1.3",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.css **/*.md **/*.json",
    "now-build": "react-scripts build && mv build dist"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
