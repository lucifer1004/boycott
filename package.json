{
  "name": "boycott",
  "version": "0.1.0",
  "description": "A map app",
  "repository": "git@github.com:lucifer1004/boycott.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "license": "MIT",
  "dependencies": {
    "@googlemap-react/core": "0.1.3",
    "lodash": "4.17.11",
    "navi": "0.12.6",
    "qs": "6.7.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-navi": "0.12.6"
  },
  "devDependencies": {
    "@types/googlemaps": "3.30.19",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.7",
    "@types/qs": "^6.5.3",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "navi-scripts": "0.12.4",
    "prettier": "1.17.0",
    "react-scripts": "3.0.0",
    "typescript": "3.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.css **/*.md **/*.json",
    "now-build": "react-scripts build && mv build dist"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
