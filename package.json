{
  "name": "boycott",
  "version": "0.1.0",
  "description": "A map app",
  "repository": "git@github.com:lucifer1004/boycott.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "license": "MIT",
  "dependencies": {
    "google-maps-react": "2.0.2",
    "navi": "0.10.7",
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "react-navi": "0.10.6"
  },
  "devDependencies": {
    "@types/google-maps-react": "2.0.1",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "navi-scripts": "0.10.6",
    "prettier": "1.15.3",
    "react-docgen-typescript": "1.12.3",
    "react-scripts": "2.1.3",
    "react-styleguidist": "8.0.6",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.css **/*.md **/*.json",
    "now-build": "react-scripts build && mv build dist",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
